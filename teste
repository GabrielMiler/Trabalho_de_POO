#include <iostream>
#include <vector>
using namespace std;
void cadastro(){
    string nome,endereco, cpf, telefone;
    cout << "Informações de cadastro"<< endl;
    cout << "Insira aqui seu nome: ";
    cin >> nome;
    cout << "Insira aqui seu cpf: ";
    cin >> cpf; 
    cout <<"Insira aqui seu telefone: ";
    cin >> telefone;
    cout <<"Insira aqui seu endereço: ";
    cin >> endereco;
}
int main() 
{
    cadastro();
    return 0;
}
